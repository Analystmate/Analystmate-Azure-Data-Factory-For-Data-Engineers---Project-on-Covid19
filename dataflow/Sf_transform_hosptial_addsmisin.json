{
	"name": "Sf_transform_hosptial_addsmisin",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_hosptital_Addmissin_data",
						"type": "DatasetReference"
					},
					"name": "HospitalAdmissions"
				},
				{
					"dataset": {
						"referenceName": "DS_for_country_lookup",
						"type": "DatasetReference"
					},
					"name": "CountryLookup"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "SelectRequiredField"
				},
				{
					"name": "lookupCountry"
				},
				{
					"name": "Splitdailyfromweekly"
				},
				{
					"name": "SelectRequiredField2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          indicator as string,",
				"          date as string,",
				"          year_week as string,",
				"          value as string,",
				"          source as string,",
				"          url as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> HospitalAdmissions",
				"source(output(",
				"          country as string,",
				"          country_code_2_digit as string,",
				"          country_code_3_digit as string,",
				"          continent as string,",
				"          population as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CountryLookup",
				"HospitalAdmissions select(mapColumn(",
				"          country,",
				"          indicator,",
				"          {Reported date} = date,",
				"          year_week,",
				"          value,",
				"          source",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectRequiredField",
				"SelectRequiredField, CountryLookup lookup(SelectRequiredField@country == CountryLookup@country,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookupCountry",
				"SelectRequiredField2 split(indicator =='Weekly new hospital admissions per 100k' || indicator == \"Weekly new ICU admissions per 100k\",",
				"     disjoint: false) ~> Splitdailyfromweekly@(Weekly, Daily)",
				"CountryLookup select(mapColumn(",
				"          country = SelectRequiredField@country,",
				"          indicator,",
				"          {Reported date},",
				"          year_week,",
				"          value,",
				"          source,",
				"          country = CountryLookup@country,",
				"          country_code_2_digit,",
				"          country_code_3_digit,",
				"          continent,",
				"          population",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectRequiredField2"
			]
		}
	}
}